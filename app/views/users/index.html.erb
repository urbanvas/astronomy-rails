<p id="notice"><%= notice %></p>

<h1>Users</h1>

<% @users.each do |user| %>
    <%= user.username %>
    <%= user.email %>
    <%= user.password %>
    <%= link_to "Show", user %>
    <%= link_to "Edit", edit_user_path(user) %>
    <%= link_to "Destroy", user, method: :delete, data: {confirm: "Sure?"} %>
    <br>

<% end %>

<%= link_to "New user", new_user_path %>